   // deleted a subregistrant.
   ACTIVATE lPrimReg WHERE lPrimReg.PrimaryRegistrant.ID = lID
   NAME VIEW lPrimReg "lPrimReg"

   SetDynamicBannerName( ViewToWindow, "wPrimR", "PrimaryRegistrant" )

