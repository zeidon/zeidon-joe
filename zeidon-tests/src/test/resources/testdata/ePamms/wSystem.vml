   END

   LOOP WHILE lMove > 0
      SET CURSOR NEXT mTempEPA.EPA_Claim
      IF RESULT < 0 OR mTempEPA.Fungi EXISTS
         lMove = lMove - 1
      END
   END

   MoveSubobject( mTempEPA, "EPA_Claim",
                  mEPA, "EPA_Claim",
                  zPOS_NEXT, zREPOS_NEXT )
   DropView( mTempEPA )

   // We now accept the Master Label to maintain order!
   COMMIT mEPA

END

